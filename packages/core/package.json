{
	"name": "wandler",
	"version": "1.0.0-alpha.1",
	"description": "Run any AI model in your browser with WebGPU - no server required",
	"type": "module",
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.umd.cjs"
		},
		"./worker": {
			"types": "./dist/worker/worker.d.ts",
			"import": "./dist/worker/worker.mjs",
			"require": "./dist/worker/worker.cjs"
		}
	},
	"files": [
		"dist",
		"dist/worker"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"test:unit": "jest tests/unit",
		"test:browser": "jest tests/browser",
		"test": "npm run test:unit && npm run test:browser"
	},
	"keywords": [
		"ai",
		"machine-learning",
		"transformers",
		"browser",
		"webgpu"
	],
	"author": "Tim Pietrusky",
	"license": "MIT",
	"dependencies": {
		"@huggingface/transformers": "^3.3.1"
	},
	"peerDependencies": {
		"@huggingface/transformers": "^3.0.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.11",
		"@webgpu/types": "^0.1.40",
		"jest": "^29.7.0",
		"puppeteer": "^21.7.0",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3",
		"vite": "^5.4.14",
		"web-worker": "^1.2.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
